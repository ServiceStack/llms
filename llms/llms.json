{
  "auth": {
    "enabled": false,
    "github": {
      "client_id": "GITHUB_CLIENT_ID",
      "client_secret": "GITHUB_CLIENT_SECRET",
      "redirect_uri": "http://localhost:8000/auth/github/callback",
      "restrict_to": "GITHUB_USERS"
    }
  },
  "defaults": {
    "headers": {
      "Content-Type": "application/json",
      "User-Agent": "llmspy.org/1.0"
    },
    "text": {
      "model": "kimi-k2",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "text",
              "text": ""
            }
          ]
        }
      ]
    },
    "image": {
      "model": "qwen2.5vl",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "image_url",
              "image_url": {
                "url": ""
              }
            },
            {
              "type": "text",
              "text": "Describe the key features of the input image"
            }
          ]
        }
      ]
    },
    "audio": {
      "model": "gpt-4o-audio-preview",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "input_audio",
              "input_audio": {
                "data": "",
                "format": "mp3"
              }
            },
            {
              "type": "text",
              "text": "Please transcribe and summarize this audio file"
            }
          ]
        }
      ]
    },
    "file": {
      "model": "qwen2.5vl",
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "file",
              "file": {
                "filename": "",
                "file_data": ""
              }
            },
            {
              "type": "text",
              "text": "Please summarize this document"
            }
          ]
        }
      ]
    },
    "check": {
      "messages": [
        {
          "role": "user",
          "content": [
            {
              "type": "text",
              "text": "1+1="
            }
          ]
        }
      ],
      "max_completion_tokens": 16,
      "stream": false
    }
  },
  "limits": {
    "client_max_size": 20971520
  },
  "convert": {
    "image": {
      "max_size": "1536x1024",
      "max_length": 1572864
    }
  },
  "providers": {
    "groq": {
      "enabled": true,
      "npm": "@ai-sdk/groq",
      "api": "https://api.groq.com/openai/v1",
      "env": ["GROQ_API_KEY"],
      "model_map": {
        "llama-3.1-8b-instant": "llama-3.1-8b-instant",
        "llama-3.3-70b-versatile": "llama-3.3-70b-versatile",
        "llama-4-scout-17b-16e-instruct": "meta-llama/llama-4-scout-17b-16e-instruct",
        "llama-4-maverick-17b-128e-instruct": "meta-llama/llama-4-maverick-17b-128e-instruct",
        "llama-guard-4-12b": "meta-llama/llama-guard-4-12b",
        "kimi-k2-instruct": "moonshotai/kimi-k2-instruct",
        "gpt-oss-20b": "openai/gpt-oss-20b",
        "gpt-oss-120b": "openai/gpt-oss-120b",
        "qwen3-32b": "qwen/qwen3-32b"
      },
      "default_pricing": {
        "input": "0",
        "output": "0"
      }
    },
    "google_free": {
      "enabled": true,
      "npm": "@ai-sdk/google",
      "env": ["GEMINI_FREE_API_KEY"],
      "model_map": {
        "gemini-flash-latest": "gemini-flash-latest",
        "gemini-flash-lite-latest": "gemini-flash-lite-latest",
        "gemini-2.5-pro": "gemini-2.5-pro",
        "gemini-2.5-flash": "gemini-2.5-flash",
        "gemini-2.5-flash-lite": "gemini-2.5-flash-lite"
      },
      "default_pricing": {
        "input": "0",
        "output": "0"
      },
      "safety_settings": [
        {
          "category": "HARM_CATEGORY_DANGEROUS_CONTENT",
          "threshold": "BLOCK_ONLY_HIGH"
        }
      ],
      "thinking_config": {
        "thinkingBudget": 1024,
        "includeThoughts": true
      },
      "check": {
        "messages": [
          {
            "role": "user",
            "content": [
              {
                "type": "text",
                "text": "1+1="
              }
            ]
          }
        ],
        "max_completion_tokens": 512,
        "reasoning": {
          "max_tokens": 128,
          "reasoning_effort": "low"
        },
        "stream": false
      }
    },
    "codestral": {
      "enabled": true,
      "npm": "codestral",
      "api": "https://codestral.mistral.ai/v1",
      "env": ["CODESTRAL_API_KEY"],
      "model_map": {
        "codestral-latest": "codestral-latest"
      },
      "default_pricing": {
        "input": "0",
        "output": "0"
      },
      "check": {
        "messages": [
          {
            "role": "user",
            "content": [
              {
                "type": "text",
                "text": "1+1="
              }
            ]
          }
        ],
        "stream": false
      }
    },
    "github-copilot": {
      "enabled": true,
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://api.githubcopilot.com",
      "env": ["GITHUB_TOKEN"],
      "all_models": true,
      "default_pricing": {
        "input": "0",
        "output": "0"
      }
    },
    "github-models": {
      "enabled": true,
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://models.github.ai/inference",
      "env": ["GITHUB_TOKEN"],
      "all_models": true,
      "default_pricing": {
        "input": "0",
        "output": "0"
      }
    },
    "openrouter_free": {
      "enabled": true,
      "npm": "openrouter_free",
      "api": "https://openrouter.ai/api/v1",
      "env": ["OPENROUTER_API_KEY"],
      "all_models": true,
      "default_pricing": {
        "input": "0",
        "output": "0"
      }
    },
    "ollama": {
      "enabled": false,
      "npm": "ollama",
      "api": "http://localhost:11434",
      "all_models": true,
      "default_pricing": {
        "input": "0",
        "output": "0"
      }
    },
    "google": {
      "enabled": true,
      "npm": "@ai-sdk/google",
      "env": ["GEMINI_API_KEY"],
      "model_map": {
        "gemini-flash-latest": "gemini-flash-latest",
        "gemini-flash-lite-latest": "gemini-flash-lite-latest",
        "gemini-2.5-pro": "gemini-2.5-pro",
        "gemini-2.5-flash": "gemini-2.5-flash",
        "gemini-2.5-flash-lite": "gemini-2.5-flash-lite"
      },
      "safety_settings": [
        {
          "category": "HARM_CATEGORY_DANGEROUS_CONTENT",
          "threshold": "BLOCK_ONLY_HIGH"
        }
      ],
      "thinking_config": {
        "thinkingBudget": 1024,
        "includeThoughts": true
      },
      "check": {
        "messages": [
          {
            "role": "user",
            "content": [
              {
                "type": "text",
                "text": "1+1="
              }
            ]
          }
        ],
        "max_completion_tokens": 512,
        "reasoning": {
          "max_tokens": 128,
          "reasoning_effort": "low"
        },
        "stream": false
      }
    },
    "anthropic": {
      "enabled": true,
      "npm": "@ai-sdk/anthropic",
      "api": "https://api.anthropic.com/v1",
      "env": ["ANTHROPIC_API_KEY"],
      "all_models": true,
      "check": {
        "messages": [
          {
            "role": "user",
            "content": [
              {
                "type": "text",
                "text": "1+1="
              }
            ]
          }
        ],
        "max_completion_tokens": 512,
        "reasoning": {
          "max_tokens": 128,
          "reasoning_effort": "low"
        },
        "stream": false
      }
    },
    "openai": {
      "enabled": true,
      "npm": "@ai-sdk/openai",
      "api": "https://api.openai.com/v1",
      "env": ["OPENAI_API_KEY"],
      "all_models": true
    },
    "xai": {
      "enabled": true,
      "npm": "@ai-sdk/xai",
