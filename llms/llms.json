    },
    "mistral": {
      "enabled": true,
      "npm": "@ai-sdk/mistral",
      "api": "https://api.mistral.ai/v1",
      "env": ["MISTRAL_API_KEY"],
      "all_models": true,
      "check": {
        "messages": [
          {
            "role": "user",
