      "api": "https://api.minimax.io/anthropic/v1",
      "env": ["MINIMAX_API_KEY"],
      "all_models": true,
      "temperature": 1.0
    },
    "zai-coding-plan": {
      "enabled": true,
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://api.z.ai/api/coding/paas/v4",
      "env": ["ZHIPU_API_KEY"],
      "all_models": true,
      "temperature": 0.7
    },
    "zai": {
      "enabled": true,
      "npm": "@ai-sdk/openai-compatible",
      "api": "https://api.z.ai/api/paas/v4",
      "env": ["ZHIPU_API_KEY"],
      "all_models": true,
